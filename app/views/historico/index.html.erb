<!DOCTYPE html>

<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>

<html>

  <head>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all',
                                              'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <!--[if lt IE 9]>
      <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/r29/html5.min.js">
      </script>
    <![endif]-->
  </head>
  <body>
    <nav class="navbar navbar-default navbar-fixed-top navbar-inverse">
      <div class="container-fluid">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/inicial/index">SIDBBA</a>
        </div>
		
      <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

          <ul class="nav navbar-nav navbar-right">

            <li><%= link_to 'Log out', destroy_user_session_path, :method => :delete %>  </li>
          </ul>
		  <ul class="nav navbar-nav" style="margin-left: 1090px;margin-top: 8px;">

            <%= link_to '/alertas_pessoais/index', class: "btn btn-default" do %>
             <i class="glyphicon glyphicon-pushpin"></i> 
            <% end %>
          </ul>
          <ul class="nav navbar-nav navbar-right" style="margin-top: 8px;">

            <%= link_to '/alertas/index', class: "btn btn-default" do %>
             <i class="glyphicon glyphicon-bell"></i> 
            <% end %>
          </ul>
        </div>
      </div>
    </nav>

<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>

<h1>Histórico</h1>

<div class="row">
  <div class="col-sm-6">
         <%=line_chart historico_historicoTemperatura_path(id: params[:id]),{library: {hAxis: {title: "Dias"}, vAxis: {title: "Graus"},title: "Temperatura"}}%>
  </div>

  <div class="col-sm-6">
         <%=line_chart historico_historicoHumidade_path(id: params[:id]),{library: {hAxis: {title: "Dias"}, vAxis: {title: "%"},title: "Humidade"}}%>
  </div>
	
</div>

<div> 
<p></p>
</div>

<div class="row">
  <div class="col-sm-6">
         <%=line_chart historico_historicoLuminosidade_path(id: params[:id]),{library: {hAxis: {title: "Dias"}, vAxis: {title: "Candela"},title: "Luminosidade"}}%>
  </div>
  <div class="col-sm-6">
         <%=line_chart historico_historicoPressao_path(id: params[:id]),{library: {hAxis: {title: "Dias"}, vAxis: {title: "mb"},title: "Pressão"}}%>
  </div>
  <div class="clearfix"></div>

 </div>
 
 </body>
</html>
<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>

<h1>Historico</h1>

<div class="row">
  <div class="col-sm-6">
     <strong>Temperaturas:</strong>
         <%=line_chart historico_historicoTemperatura_path(id: params[:id])%>
  </div>

  <div class="col-sm-6">
     <strong>Humidade:</strong>
         <%=line_chart historico_historicoHumidade_path(id: params[:id])%>
  </div>
	
 </div>

<div class="row">
  <div class="col-sm-6">
     <strong>Ruido:</strong>
         <%=line_chart historico_historicoRuido_path(id: params[:id])%>
  </div>

  
 </div>